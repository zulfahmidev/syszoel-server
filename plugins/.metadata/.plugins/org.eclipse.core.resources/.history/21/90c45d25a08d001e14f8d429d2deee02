<?php

namespace SysCore\PClan;

use pocketmine\player\Player;

class Clan {
    
    private $clanId;
    private $data;
    
    public function __construct($clanId, $data) {
        $this->clanId = $clanId;
        $this->data = $data;
    }
    
    public function getId() {
        return $this->clanId;
    }
    
    public function getName() {
        return $this->data['name'];
    }
    
}